{
  "multipart": [
    {
      "when": {
        "axis": "x"
      },
      "apply": {
        "model": "more_stuff:block/red_blaze_brick_pillar",
        "x": 90,
        "y": 90
      }
    },
    {
      "when": {
        "axis": "y"
      },
      "apply": {
        "model": "more_stuff:block/red_blaze_brick_pillar"
      }
    },
    {
      "when": {
        "axis": "z"
      },
      "apply": {
        "model": "more_stuff:block/red_blaze_brick_pillar",
        "x": 90
      }
    }
  ]
}
