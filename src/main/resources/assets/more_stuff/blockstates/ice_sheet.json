{
  "multipart": [
    {
      "when": { "down": "true" },
      "apply": { "model": "more_stuff:block/ice_sheet" }
    },
    {
      "when": { "up": "true" },
      "apply": {
        "model": "more_stuff:block/ice_sheet",
        "x": 180
      }
    },
    {
      "when": { "north": "true" },
      "apply": {
        "model": "more_stuff:block/ice_sheet",
        "x": 90,
        "y": 180
      }
    },
    {
      "when": { "south": "true" },
      "apply": {
        "model": "more_stuff:block/ice_sheet",
        "x": 90,
        "y": 0
      }
    },
    {
      "when": { "east": "true" },
      "apply": {
        "model": "more_stuff:block/ice_sheet",
        "x": 90,
        "y": 270
      }
    },
    {
      "when": { "west": "true" },
      "apply": {
        "model": "more_stuff:block/ice_sheet",
        "x": 90,
        "y": 90
      }
    }
  ]
}